@camera persp
$pos 0.0 0.0 150.0
:camera_push "persp"

@voxel dragon
$pos -80.0 0.0 0.0
$rot 0.0 -0.1 0.0
$file "model/dragon.vox"

@voxel teapot
$pos 80.0 40.0 0.0
$rot -1.0 -0.0 -0.0
$file "model/teapot.vox"

@voxel plane0
$pos -50.0 -50.0 0.0
:addPlane 100 100 0xFF0000FF

@voxel plane1
$pos 50.0 -50.0 0.0
:addPlane 100 100 0x00FF00FF

@voxel plane2
$pos 0.0 -50.0 -50.0
:addPlane 100 100 0x0000FFFF

@voxel plane3
$pos 0.0 -50.0 50.0
:addPlane 100 100 0x666666FF

